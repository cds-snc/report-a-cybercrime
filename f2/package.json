{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.4.1",
    "@emotion/core": "^10.0.17",
    "@emotion/styled": "^10.0.17",
    "@lingui/react": "^3.0.0-7",
    "@reach/component-component": "^0.5.0",
    "babel-plugin-macros": "^2.6.2",
    "body-parser": "^1.19.0",
    "chroma-js": "^2.1.0",
    "core-js": "^3.4.8",
    "dotenv": "^8.2.0",
    "emotion-theming": "^10.0.19",
    "express": "^4.17.1",
    "final-form": "^4.18.6",
    "graphql": "^14.5.7",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "mongodb": "^3.3.3",
    "npm-check": "^5.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.3.3",
    "react-ga": "^2.6.0",
    "react-intl": "^3.11.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "styled-system": "^5.1.2"
  },
  "scripts": {
    "prod": "npm run build && node server.js",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "watch": "react-scripts test",
    "dbg": "react-scripts --inspect-brk test --watchAll=false --runInBand",
    "eject": "react-scripts eject",
    "dev": "react-scripts start",
    "lint": "eslint --max-warnings=0 'src/**/*.js'",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract --clean",
    "compile": "lingui compile",
    "upgrade-interactive": "npm-check --update"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@lingui/cli": "^3.0.0-7",
    "@lingui/loader": "^3.0.0-7",
    "@lingui/macro": "^3.0.0-7",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "babel-core": "^7.0.0-bridge.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "jest-emotion": "^10.0.17",
    "recompose": "^0.30.0",
    "waait": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
