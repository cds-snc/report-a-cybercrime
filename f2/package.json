{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@azure/storage-blob": "^12.1.1",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@lingui/react": "^3.0.0-7",
    "async": "^3.2.0",
    "azure-cognitiveservices-contentmoderator": "^4.1.1",
    "babel-plugin-macros": "^2.8.0",
    "body-parser": "^1.19.0",
    "child_process": "^1.0.2",
    "chroma-js": "^2.1.0",
    "clamdjs": "^1.0.2",
    "core-js": "^3.6.4",
    "date-fns": "^2.12.0",
    "dotenv": "^8.2.0",
    "email-addresses": "^3.1.0",
    "emotion-theming": "^10.0.27",
    "express": "^4.17.1",
    "express-rate-limit": "^5.1.1",
    "express-winston": "4.0.3",
    "final-form": "^4.19.0",
    "flat": "^5.0.0",
    "formidable": "^1.2.2",
    "get-user-locale": "^1.3.0",
    "helmet": "^3.21.3",
    "isomorphic-unfetch": "^3.0.0",
    "jimp": "0.12.1",
    "ldapjs": "^1.0.2",
    "mongodb": "^3.5.3",
    "ms-rest-azure": "^3.0.0",
    "nanoid": "^2.1.11",
    "natural": "^2.1.5",
    "nodemailer": "^6.4.6",
    "notifications-node-client": "^4.7.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-final-form": "^6.4.0",
    "react-ga": "^2.6.0",
    "react-google-recaptcha-v3": "^1.4.1",
    "react-gtm-module": "^2.0.8",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.1",
    "sanitize-html": "^1.22.1",
    "speakeasy": "^2.0.0",
    "styled-system": "^5.1.5",
    "unidecode": "^0.1.8",
    "winston": "~3.2.1",
    "zipcodes": "^8.0.0"
  },
  "scripts": {
    "prod": "npm run build && node server.js",
    "build": "react-scripts build",
    "start": "NODE_ENV=production node server.js",
    "test": "SELF_HARM_WORDS= react-scripts test --watchAll=false",
    "coverage": "SELF_HARM_WORDS= react-scripts test --coverage --watchAll=false",
    "watch": "react-scripts test",
    "dbg": "react-scripts --inspect-brk test --watchAll=false --runInBand",
    "eject": "react-scripts eject",
    "start:cypress": "npm run dev",
    "cypress:open": "node ./node_modules/.bin/cypress open ",
    "cypress": "start-server-and-test start:cypress http://localhost:3000 cypress:open",
    "cypress:smoke": "node ./node_modules/.bin/cypress-tags run -e TAGS='@smoke'",
    "cypress:consent": "node ./node_modules/.bin/cypress-tags run -e TAGS='@consent'",
    "cypress:cli": "start-server-and-test http://localhost:3000 cypress:smoke",
    "clean:reports": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "dev": "react-scripts start",
    "lint": "eslint --max-warnings=0 'src/**/*.js'",
    "add-locale": "lingui add-locale",
    "extract": "lingui extract --clean",
    "compile": "lingui compile",
    "check-translations": "node utils/checkTranslations.js",
    "upgrade-interactive": "npm-check --update",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "deploy-storybook": "storybook-to-ghpages"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo 'Running npm run extract' && lingui extract --clean && git add src/locales && pretty-quick --staged"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/polyfill": "^7.8.7",
    "@cdssnc/a11y-tracker-client": "^1.1.2",
    "@lingui/cli": "^3.0.0-7",
    "@lingui/loader": "^3.0.0-7",
    "@lingui/macro": "^3.0.0-7",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/eslint-config-storybook": "^2.2.0",
    "@storybook/preset-create-react-app": "^1.5.2",
    "@storybook/react": "^5.3.17",
    "@storybook/storybook-deployer": "^2.8.3",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.0",
    "axe-core": "^3.5.2",
    "babel-core": "^7.0.0-bridge.0",
    "chance": "^1.1.3",
    "cypress": "^4.3.0",
    "cypress-cucumber-preprocessor": "^2.0.1",
    "cypress-file-upload": "^3.5.3",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "husky": "^4.2.3",
    "jest-emotion": "^10.0.32",
    "mocha": "^7.0.1",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^4.0.3",
    "mochawesome-report-generator": "^4.1.0",
    "npm-check": "^5.9.2",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "react-is": "^16.13.1",
    "recompose": "^0.30.0",
    "start-server-and-test": "^1.10.11",
    "storybook-addon-jsx": "^7.1.15",
    "waait": "^1.0.5"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 50,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
