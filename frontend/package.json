{
  "name": "my-razzle-app",
  "version": "2.0.0-alpha.8",
  "license": "MIT",
  "scripts": {
    "dev": "razzle start",
    "build": "razzle build",
    "test": "jest",
    "postinstall": "razzle build",
    "lint": "eslint 'src/**/*.js'",
    "start": "NODE_ENV=production node build/server.js",
